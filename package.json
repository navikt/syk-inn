{
    "name": "syk-inn",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@4.12.0",
    "engines": {
        "node": "24"
    },
    "workspaces": [
        "libs/*",
        "e2e"
    ],
    "scripts": {
        "dev": "./scripts/pre-dev.sh && concurrently --names 'next,gen'  --prefix-colors 'blue,magenta' 'yarn dev:next' 'yarn gen:watch'",
        "dev:next": "next dev --turbo | pino-pretty --messageKey=message",
        "dev:valkey": "docker run --name syk-inn-dev-valkey -p 6379:6379 -d valkey/valkey:latest",
        "build": "./scripts/pre-build.sh && next build",
        "build:e2e": "./scripts/build-e2e.sh",
        "test": "vitest --project unit --run | pino-pretty --messageKey=message",
        "test:integration": "vitest --project integration --run | pino-pretty --messageKey=message",
        "lint": "eslint src",
        "e2e": "./scripts/pre-e2e.sh && yarn workspace e2e run e2e",
        "e2e:dev": "yarn e2e --ui",
        "e2e:fast": "echo 'Did you run yarn build:e2e? before this?' && FAST=true yarn e2e",
        "next:e2e:start": "NEXT_PUBLIC_ASSET_PREFIX= node .next/standalone/server.js | pino-pretty --messageKey=message",
        "next:e2e:dev": "./scripts/pre-e2e-dev.sh && yarn dev:next",
        "gen": "graphql-codegen",
        "gen:watch": "graphql-codegen --watch",
        "postinstall": "husky"
    },
    "dependencies": {
        "@apollo/client": "^4.0.9",
        "@ariakit/react": "^0.4.19",
        "@envelop/core": "^5.4.0",
        "@envelop/opentelemetry": "^9.0.0",
        "@grafana/faro-web-sdk": "^1.19.0",
        "@grafana/faro-web-tracing": "^1.19.0",
        "@graphql-tools/schema": "^10.0.29",
        "@navikt/aksel-icons": "^7.34.0",
        "@navikt/diagnosekoder": "^1.2025.0",
        "@navikt/ds-css": "^7.34.0",
        "@navikt/ds-react": "^7.34.0",
        "@navikt/ds-tailwind": "^7.34.0",
        "@navikt/fhir-mock-server": "workspace:*",
        "@navikt/helseid-mock-server": "workspace:*",
        "@navikt/next-api-proxy": "^5.0.0",
        "@navikt/next-logger": "^4.4.1",
        "@navikt/smart-on-fhir": "1.5.13",
        "@navikt/texas": "^4.1.1",
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/core": "^2.2.0",
        "@reduxjs/toolkit": "^2.10.1",
        "@unleash/nextjs": "^1.6.2",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "fuse.js": "^7.1.0",
        "graphql": "^16.12.0",
        "graphql-yoga": "^5.16.2",
        "iovalkey": "^0.3.3",
        "jose": "^6.1.2",
        "motion": "^12.23.24",
        "next": "16.0.3",
        "next-logger": "^5.0.2",
        "nextleton": "^0.6.1",
        "nuqs": "^2.8.1",
        "pino": "^10.1.0",
        "pino-pretty": "^13.1.2",
        "pino-socket": "^7.4.0",
        "prom-client": "^15.1.3",
        "quick-lru": "^7.3.0",
        "react": "19.2.0",
        "react-dom": "19.2.0",
        "react-hook-form": "^7.66.1",
        "react-minimal-pie-chart": "^9.1.2",
        "react-redux": "^9.2.0",
        "remeda": "^2.32.0",
        "rxjs": "^7.8.2",
        "sonner": "^2.0.7",
        "tailwind-merge": "^3.4.0",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^6.1.0",
        "@graphql-codegen/fragment-matcher": "^6.0.0",
        "@graphql-codegen/introspection": "^5.0.0",
        "@graphql-codegen/typescript-resolvers": "^5.1.3",
        "@hookform/devtools": "^4.4.0",
        "@limegrass/eslint-plugin-import-alias": "^1.6.0",
        "@navikt/tsm-eslint-react": "^1.0.0",
        "@navikt/tsm-prettier": "^2.0.0",
        "@parcel/watcher": "^2.5.1",
        "@tailwindcss/postcss": "^4.1.17",
        "@testcontainers/kafka": "^11.8.1",
        "@testcontainers/postgresql": "^11.8.1",
        "@types/node": "^24.10.1",
        "@types/react": "19.2.6",
        "@types/react-dom": "19.2.3",
        "concurrently": "^9.2.1",
        "eslint": "^9.39.1",
        "eslint-config-next": "16.0.3",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "husky": "^9.1.7",
        "kafkajs": "^2.2.4",
        "knip": "^5.70.1",
        "lint-staged": "^16.2.7",
        "nock": "^14.0.10",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "sonda": "^0.10.0",
        "tailwindcss": "^4.1.17",
        "testcontainers": "^11.8.1",
        "typescript": "^5.9.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^4.0.13"
    },
    "prettier": "@navikt/tsm-prettier"
}
