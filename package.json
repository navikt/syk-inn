{
    "name": "syk-inn",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@4.9.2",
    "engines": {
        "node": "22"
    },
    "workspaces": [
        "libs/*"
    ],
    "scripts": {
        "dev": "./scripts/pre-dev.sh && concurrently --names 'next,gen'  --prefix-colors 'blue,magenta' 'yarn dev:next' 'yarn gen:watch'",
        "dev:next": "next dev --turbo | pino-pretty --messageKey=message",
        "dev:valkey": "docker run --name syk-inn-dev-valkey -p 6379:6379 -d valkey/valkey:latest",
        "build": "./scripts/pre-build.sh && next build",
        "build:e2e": "./scripts/build-e2e.sh",
        "start": "next start",
        "start:e2e": "NEXT_PUBLIC_ASSET_PREFIX= node .next/standalone/server.js | pino-pretty --messageKey=message",
        "test": "vitest",
        "lint": "next lint",
        "e2e": "./scripts/pre-e2e.sh && playwright test",
        "e2e:dev": "yarn e2e --ui",
        "dev:e2e": "yarn e2e:dev",
        "e2e:fast": "echo 'Did you run yarn build:e2e? before this?' && FAST=true yarn e2e",
        "gen": "graphql-codegen",
        "gen:watch": "graphql-codegen --watch",
        "postinstall": "husky"
    },
    "dependencies": {
        "@apollo/client": "^3.13.8",
        "@apollo/client-integration-nextjs": "^0.12.2",
        "@apollo/server": "^4.12.2",
        "@ariakit/react": "^0.4.17",
        "@as-integrations/next": "^3.2.0",
        "@grafana/faro-web-sdk": "^1.19.0",
        "@grafana/faro-web-tracing": "^1.19.0",
        "@graphql-tools/graphql-file-loader": "^8.0.20",
        "@graphql-tools/load": "^8.1.0",
        "@graphql-tools/schema": "^10.0.23",
        "@navikt/aksel-icons": "^7.25.1",
        "@navikt/diagnosekoder": "^1.2025.0",
        "@navikt/ds-css": "^7.25.1",
        "@navikt/ds-react": "^7.25.1",
        "@navikt/ds-tailwind": "^7.25.1",
        "@navikt/fhir-mock-server": "workspace:*",
        "@navikt/next-api-proxy": "^4.1.4",
        "@navikt/next-logger": "^4.1.5",
        "@navikt/oasis": "^3.8.0",
        "@navikt/smart-on-fhir": "1.2.2",
        "@opentelemetry/api": "^1.9.0",
        "@reduxjs/toolkit": "^2.8.2",
        "@unleash/nextjs": "^1.6.2",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "fuse.js": "^7.1.0",
        "graphql": "^16.11.0",
        "iovalkey": "^0.3.3",
        "motion": "^12.23.3",
        "next": "15.3.5",
        "next-logger": "^5.0.1",
        "nextleton": "^0.6.1",
        "node-cache": "^5.1.2",
        "nuqs": "^2.4.3",
        "pino": "^9.7.0",
        "pino-pretty": "^13.0.0",
        "pino-socket": "^7.4.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-hook-form": "^7.60.0",
        "react-redux": "^9.2.0",
        "remeda": "^2.25.0",
        "server-only": "^0.0.1",
        "sonner": "^2.0.6",
        "tailwind-merge": "^3.3.1",
        "zod": "^4.0.5"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^5.0.7",
        "@graphql-codegen/fragment-matcher": "^5.1.0",
        "@graphql-codegen/typescript-resolvers": "^4.5.1",
        "@hookform/devtools": "^4.4.0",
        "@navikt/eslint-config-teamsykmelding": "^7.1.0",
        "@parcel/watcher": "^2.5.1",
        "@playwright/test": "1.54.1",
        "@tailwindcss/postcss": "^4.1.11",
        "@types/node": "^22.16.3",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "concurrently": "^9.2.0",
        "eslint": "^8.57.1",
        "eslint-config-next": "15.3.5",
        "husky": "^9.1.7",
        "knip": "^5.61.3",
        "lint-staged": "^16.1.2",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "sonda": "^0.9.0",
        "tailwindcss": "^4.1.11",
        "typescript": "^5.8.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ]
    }
}
