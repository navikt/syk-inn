{
    "name": "syk-inn",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@4.5.3",
    "engines": {
        "node": "22"
    },
    "scripts": {
        "dev": "./scripts/pre-dev.sh && next dev --turbo | pino-pretty",
        "dev:redis": "docker run --name syk-inn-dev-redis -p 6379:6379 -d redis",
        "build": "./scripts/pre-build.sh && next build",
        "build:e2e": "./scripts/build-e2e.sh",
        "start": "next start",
        "start:e2e": "NEXT_PUBLIC_ASSET_PREFIX= node .next/standalone/server.js | pino-pretty",
        "test": "vitest",
        "lint": "next lint",
        "e2e": "playwright test",
        "e2e:dev": "yarn e2e --ui",
        "e2e:fast": "FAST=true yarn e2e --ui",
        "prepare": "husky"
    },
    "dependencies": {
        "@ariakit/react": "^0.4.15",
        "@navikt/aksel-icons": "^7.8.1",
        "@navikt/diagnosekoder": "^1.2025.0",
        "@navikt/ds-css": "^7.8.1",
        "@navikt/ds-react": "^7.8.1",
        "@navikt/ds-tailwind": "^7.8.1",
        "@navikt/next-logger": "^1.35.0",
        "@navikt/oasis": "^3.6.0",
        "@redis/client": "^1.6.0",
        "@tanstack/react-query": "^5.62.8",
        "@tanstack/react-query-devtools": "^5.62.8",
        "clsx": "^2.1.1",
        "csp-header": "^6.0.0",
        "date-fns": "^4.1.0",
        "fhirclient": "^2.5.4",
        "fuse.js": "^7.0.0",
        "html-react-parser": "^5.2.1",
        "jose": "^5.9.6",
        "jsdom": "^25.0.1",
        "next": "15.1.2",
        "next-logger": "^5.0.1",
        "nextleton": "^0.6.1",
        "nuqs": "^2.2.3",
        "pino": "^9.5.0",
        "pino-pretty": "^13.0.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-error-boundary": "^4.1.2",
        "react-hook-form": "^7.54.1",
        "remeda": "^2.18.0",
        "server-only": "^0.0.1",
        "tailwind-merge": "^2.5.5",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@hookform/devtools": "^4.3.1",
        "@navikt/eslint-config-teamsykmelding": "^7.1.0",
        "@playwright/test": "1.49.1",
        "@types/node": "^22.10.2",
        "@types/react": "^19.0.2",
        "@types/react-dom": "^19.0.2",
        "autoprefixer": "^10.4.20",
        "eslint": "^8.57.1",
        "eslint-config-next": "15.1.2",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.11",
        "postcss": "^8.4.49",
        "prettier": "^3.4.2",
        "tailwindcss": "^3.4.17",
        "typescript": "^5.7.2",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^2.1.8"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ]
    }
}
