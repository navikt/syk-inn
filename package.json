{
    "name": "syk-inn",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@4.5.1",
    "scripts": {
        "dev": "./scripts/pre-dev.sh && next dev --turbo | pino-pretty",
        "dev:redis": "docker run --name syk-inn-dev-redis -p 6379:6379 -d redis",
        "build": "./scripts/pre-build.sh && next build",
        "build:e2e": "./scripts/build-e2e.sh",
        "start": "next start",
        "start:e2e": "NEXT_PUBLIC_ASSET_PREFIX= node .next/standalone/server.js | pino-pretty",
        "test": "vitest",
        "lint": "next lint",
        "e2e": "playwright test",
        "e2e:dev": "yarn e2e --ui",
        "e2e:fast": "FAST=true yarn e2e --ui",
        "prepare": "husky"
    },
    "dependencies": {
        "@ariakit/react": "^0.4.13",
        "@navikt/aksel-icons": "^7.4.0",
        "@navikt/diagnosekoder": "^1.2024.1",
        "@navikt/ds-css": "^7.4.0",
        "@navikt/ds-react": "^7.4.0",
        "@navikt/ds-tailwind": "^7.4.0",
        "@navikt/nav-dekoratoren-moduler": "^3.1.1",
        "@navikt/next-logger": "^1.34.0",
        "@redis/client": "^1.6.0",
        "@tanstack/react-query": "^5.59.16",
        "@tanstack/react-query-devtools": "^5.59.16",
        "clsx": "^2.1.1",
        "csp-header": "^5.2.1",
        "date-fns": "^3.6.0",
        "fhirclient": "^2.5.4",
        "fuse.js": "^7.0.0",
        "html-react-parser": "^5.1.18",
        "jose": "^5.9.6",
        "jsdom": "^25.0.1",
        "next": "15.0.1",
        "next-logger": "^5.0.1",
        "nextleton": "^0.6.1",
        "nuqs": "^2.0.4",
        "pino": "^9.5.0",
        "pino-pretty": "^11.3.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-error-boundary": "^4.1.2",
        "react-hook-form": "^7.53.1",
        "remeda": "^2.16.0",
        "tailwind-merge": "^2.5.4",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@hookform/devtools": "^4.3.1",
        "@navikt/eslint-config-teamsykmelding": "^7.0.0",
        "@playwright/test": "^1.48.2",
        "@types/node": "^20.17.1",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "autoprefixer": "^10.4.20",
        "eslint": "^8.57.1",
        "eslint-config-next": "15.0.1",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "postcss": "^8.4.47",
        "prettier": "^3.3.3",
        "tailwindcss": "^3.4.14",
        "typescript": "^5.6.3",
        "vitest": "^2.1.3"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ]
    }
}
