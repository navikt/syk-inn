{
    "name": "syk-inn",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@4.9.1",
    "engines": {
        "node": "22"
    },
    "workspaces": [
        "libs/*"
    ],
    "scripts": {
        "dev": "./scripts/pre-dev.sh && next dev --turbo | pino-pretty",
        "dev:valkey": "docker run --name syk-inn-dev-valkey -p 6379:6379 -d valkey/valkey:latest",
        "build": "./scripts/pre-build.sh && next build",
        "build:e2e": "./scripts/build-e2e.sh",
        "start": "next start",
        "start:e2e": "NEXT_PUBLIC_ASSET_PREFIX= node .next/standalone/server.js | pino-pretty",
        "test": "vitest",
        "lint": "next lint",
        "e2e": "playwright test",
        "e2e:dev": "yarn e2e --ui",
        "e2e:fast": "FAST=true yarn e2e --ui",
        "postinstall": "husky"
    },
    "dependencies": {
        "@ariakit/react": "^0.4.17",
        "@navikt/aksel-icons": "^7.21.1",
        "@navikt/diagnosekoder": "^1.2025.0",
        "@navikt/ds-css": "^7.21.1",
        "@navikt/ds-react": "^7.21.1",
        "@navikt/ds-tailwind": "^7.21.1",
        "@navikt/fhir-mock-server": "workspace:*",
        "@navikt/fhir-zod": "workspace:*",
        "@navikt/next-logger": "^3.0.3",
        "@navikt/oasis": "^3.8.0",
        "@navikt/smart-on-fhir": "workspace:*",
        "@reduxjs/toolkit": "^2.7.0",
        "@tanstack/react-query": "^5.74.11",
        "@tanstack/react-query-devtools": "^5.75.7",
        "@unleash/nextjs": "^1.6.2",
        "clsx": "^2.1.1",
        "csp-header": "^6.1.0",
        "date-fns": "^4.1.0",
        "fuse.js": "^7.1.0",
        "iovalkey": "^0.3.1",
        "jose": "^6.0.10",
        "motion": "^12.9.2",
        "next": "15.3.1",
        "next-logger": "^5.0.1",
        "nextleton": "^0.6.1",
        "node-cache": "^5.1.2",
        "nuqs": "^2.4.3",
        "openid-client": "^6.4.2",
        "pino": "^9.6.0",
        "pino-pretty": "^13.0.0",
        "pino-roll": "^3.1.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-error-boundary": "^5.0.0",
        "react-hook-form": "^7.56.1",
        "react-redux": "^9.2.0",
        "remeda": "^2.21.3",
        "server-only": "^0.0.1",
        "tailwind-merge": "^3.3.0",
        "zod": "^3.24.3"
    },
    "devDependencies": {
        "@hookform/devtools": "^4.4.0",
        "@navikt/eslint-config-teamsykmelding": "^7.1.0",
        "@playwright/test": "1.52.0",
        "@tailwindcss/postcss": "^4.1.6",
        "@types/node": "^22.15.17",
        "@types/react": "^19.1.3",
        "@types/react-dom": "^19.1.3",
        "eslint": "^8.57.1",
        "eslint-config-next": "15.3.1",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.1",
        "postcss": "^8.5.3",
        "prettier": "^3.5.3",
        "tailwindcss": "^4.1.6",
        "typescript": "^5.8.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.1.3"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ]
    }
}
